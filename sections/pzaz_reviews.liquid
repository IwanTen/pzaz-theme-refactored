{{
   "reviews.css" | asset_url | stylesheet_tag }}

<div class="reviews container container--padding color-primary">

  <h1 class="section__title reviews__title">Energy<br> Your Way</h1>
  <p class="reviews__headline">Pzaz powers up creatives, athletes, students,
    professionals, travelers, and entertainers.</p>

  <div class="reviews__carousel embla">
    <div class="embla__container">
      <div class="embla__slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0685/9076/4328/files/903581E8-D2F7-4B58-93AB-7F120F04ED29.jpg?v=1671570767"
          alt="review"
          class="reviews__image">
        <div class="fixed-atop">
          {% render "zazzy-cta-button" color: "secondary" size: "small" %}
        </div>
      </div>
      <div class="embla__slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0685/9076/4328/files/F7B4C7DF-6B8F-4F5C-8DD1-D7012E889A92.jpg?v=1671570766"
          alt="review"
          class="reviews__image">
        <div class="fixed-atop">
          {% render "zazzy-cta-button" color: "secondary" size: "small" %}
        </div>
      </div>
      <div class="embla__slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0685/9076/4328/files/44BCD2C1-2A84-4A12-890C-CE2978257CE3_1_105_c.jpg?v=1671570785"
          alt="review"
          class="reviews__image">
        <div class="fixed-atop">
          {% render "zazzy-cta-button" color: "secondary" size: "small" %}
        </div>
      </div>
      <div class="embla__slide">
        <img
          src="https://cdn.shopify.com/s/files/1/0685/9076/4328/files/74EA3B61-234F-46A2-9433-57082C985ECE_1_105_c.jpg?v=1671570807"
          alt="review"
          class="reviews__image">
        <div class="fixed-atop">
          {% render "zazzy-cta-button" color: "secondary" size: "small" %}
        </div>
      </div>
    </div>
  </div>

  <h1 class="section__title map__title">Check us out</h1>
  <p class="map__headline">Find us at more than 200+ locations nationwide.</p>

</div>



{% schema %}
  {
    "name": "reviews-section",
    "tag": "section",
    "class": "section bg-dark",
    "settings": []
  }
{% endschema %}